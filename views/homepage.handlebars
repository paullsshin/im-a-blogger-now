<!-- Loop over each object in the projects array -->
<div class="posts">
{{#each posts }}
  <article class="card">
    <div>
    <h2>
      <a href="/post/{{id}}">{{title}}</a>
    </h2>
    </div>
    <div>Posted by {{User.username}} on {{format_date createdAt}}</div>
    <div class="card-body">
      {{ body }}
    </div>
  </article>
{{/each}}
</div>
